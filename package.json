{
  "name": "@bmd-studio/genstack-container-postgraphile",
  "private": true,
  "main": "src/index.js",
  "scripts": {
    "setup": "yarn install",
    "start": "NODE_OPTIONS=\"--inspect=0.0.0.0:$DEFAULT_DEBUG_PORT\" node src/index.js",
    "start:dev": "pm2-runtime pm2.config.js",
    "start:prod": "node src/index.js",
    "start:npx": "npx postgraphile -c postgres://postgres:password@postgresql:5432/proj --schema app_public --port 4000 --host 0.0.0.0",
    "test": "yarn test:integration",
    "test:debug": "cross-env DEBUG=pg-jwt-auth:* yarn test",
    "test:integration": "jest --runInBand --testPathPattern=integration/",
    "build": "tsc",
    "build:watch": "yarn build --watch",
    "clean": "rimraf dist/ && rimraf coverage/ && rimraf node_modules/",
    "dc": "docker-compose --env-file=./env/defaults -f docker-compose.yml --compatibility",
    "dc:dev": "yarn dc -f docker-compose.development.yml"
  },
  "dependencies": {
    "@bmd-studio/genstack-environment": "^2.0.3",
    "@bmd-studio/genstack-hooks": "^1.0.3",
    "@bmd-studio/genstack-logger": "^1.0.2",
    "@bmd-studio/graphql-mqtt-subscriptions": "^1.3.0",
    "@fullstackio/postgraphile-upsert-plugin": "^1.4.0",
    "@graphile-contrib/pg-many-to-many": "^1.0.0-beta.5",
    "@graphile-contrib/pg-order-by-related": "^1.0.0-beta.5",
    "@graphile-contrib/pg-simplify-inflector": "^6.0.0",
    "@graphile/subscriptions-lds": "^4.4.1",
    "body-parser": "^1.19.0",
    "change-case": "^4.1.1",
    "connect-pg-simple": "^5.0.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-bearer-token": "^2.4.0",
    "express-session": "^1.16.2",
    "graphql-subscriptions": "^1.1.0",
    "helmet": "^3.18.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.11",
    "morgan": "^1.9.1",
    "mqtt": "^3.0.0",
    "pg": "^7.11.0",
    "postgraphile": "^4.8.0",
    "postgraphile-plugin-connection-filter": "^1.1.1",
    "postgraphile-plugin-nested-mutations": "^1.0.0-alpha.23",
    "sift": "^8.5.0",
    "upath": "^1.1.2"
  },
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/lodash": "^4.14.149",
    "@types/pg": "^7.14.3",
    "@types/supertest": "^2.0.9",
    "cross-env": "^7.0.2",
    "get-port": "^5.1.1",
    "http-status-codes": "^1.4.0",
    "jest": "^25.1.0",
    "pm2": "^4.4.1",
    "rimraf": "^3.0.2",
    "supertest": "^4.0.2",
    "testcontainers": "^2.8.1",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.9.1",
    "typescript": "^3.8.3"
  }
}
