{
  "name": "@bmd-studio/genstack-container-postgraphile",
  "private": true,
  "main": "src/index.js",
  "scripts": {
    "setup": "yarn install",
    "start": "node -r ts-node/register src/index.ts",
    "start:debug": "yarn start --inspect=0.0.0.0:${DEFAULT_DEBUG_PORT:-9229}",
    "start:prod": "node dist/index.js",
    "start:pm2": "pm2-runtime pm2.config.js",
    "start:docker": "yarn build && yarn dc:dev up",
    "start:docker-prod": "yarn dc up",
    "dc": "docker-compose --env-file=./env/defaults -f docker-compose.yml --compatibility",
    "dc:dev": "cross-env NODE_ENV=development GS_ENV=development yarn dc -f docker-compose.development.yml",
    "test": "yarn test:integration",
    "test:integration": "jest --runInBand --testPathPattern=integration/",
    "build": "tsc",
    "build:watch": "yarn build --watch",
    "build:docker": "yarn dc:dev build",
    "build:docker-prod": "yarn dc build",
    "clean": "rimraf dist/ && rimraf coverage/ && rimraf node_modules/"
  },
  "dependencies": {
    "@bmd-studio/genstack-environment": "3.1.1",
    "@bmd-studio/genstack-hooks": "1.0.3",
    "@bmd-studio/graphql-mqtt-subscriptions": "1.4.2",
    "@fullstackio/postgraphile-upsert-plugin": "1.4.0",
    "@graphile-contrib/pg-many-to-many": "1.0.0",
    "@graphile-contrib/pg-order-by-related": "1.0.0-beta.6",
    "@graphile-contrib/pg-simplify-inflector": "6.0.0",
    "@graphile/subscriptions-lds": "4.9.0",
    "body-parser": "1.19.0",
    "chalk": "4.1.0",
    "change-case": "4.1.1",
    "connect-pg-simple": "5.0.0",
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "debug": "4.2.0",
    "dotenv-parse-variables": "1.0.1",
    "express": "4.17.1",
    "express-session": "1.17.1",
    "graphile-column-privileges-mutations": "0.0.6",
    "graphql-subscriptions": "1.1.0",
    "helmet": "4.2.0",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.20",
    "morgan": "1.10.0",
    "mqtt": "4.2.4",
    "pg": "7.18.2",
    "postgraphile": "4.9.2",
    "postgraphile-plugin-connection-filter": "1.1.3",
    "postgraphile-plugin-nested-mutations": "1.0.1",
    "sift": "8.5.1",
    "upath": "1.2.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "1.4.2",
    "@types/cors": "2.8.8",
    "@types/debug": "4.1.5",
    "@types/dotenv-parse-variables": "0.2.0",
    "@types/jest": "26.0.14",
    "@types/jsonwebtoken": "8.5.0",
    "@types/lodash": "4.14.161",
    "@types/morgan": "1.9.2",
    "@types/pg": "7.14.5",
    "@types/supertest": "2.0.10",
    "cross-env": "7.0.2",
    "get-port": "5.1.1",
    "http-status-codes": "1.4.0",
    "jest": "25.5.4",
    "pm2": "4.4.1",
    "rimraf": "3.0.2",
    "supertest": "4.0.2",
    "testcontainers": "2.20.0",
    "ts-jest": "25.5.1",
    "ts-node": "9.0.0",
    "typescript": "3.9.7"
  }
}
